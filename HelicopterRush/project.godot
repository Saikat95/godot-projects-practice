; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

[application]

config/name="HelicopterRush"
run/main_scene="res://Scene/StartMenu.tscn"
config/icon="res://Assets/heli_body.png"

[autoload]

Global="*res://Script/AutoLoad.gd"
Ui="*res://Scene/UI.tscn"
Ui_Script="*res://Script/UI.gd"
Music="*res://Animation/Music.tscn"

[display]

window/size/width=1280
window/size/height=720
window/size/fullscreen=true
window/stretch/mode="2d"
window/stretch/aspect="keep"

[layer_names]

2d_render/layer_1="world"
2d_physics/layer_1="world"
2d_render/layer_2="heli"
2d_physics/layer_2="heli"
2d_render/layer_3="bullet"
2d_physics/layer_3="bullet"
2d_render/layer_4="enemy"
2d_physics/layer_4="enemy"

[physics]

common/enable_pause_aware_picking=true

[rendering]

environment/default_environment="res://default_env.tres"
